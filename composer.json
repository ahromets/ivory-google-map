{
    "name": "yappabe/google-map",
    "description": "Google Map API v3 integration for PHP 5.3+",
    "keywords": [ "google map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "egeloen/json-builder": "^2.0",
        "php-http/message": "^1.3",
        "php-http/guzzle6-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "willdurand/geocoder": "~2.0",
        "satooshi/php-coveralls": "~0.6"
    },
    "suggest": {
        "willdurand/geocoder": "Allows you to use geocoding service",
        "php-http/guzzle6-adapter": "Allows you to use services"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMap\\": "src/" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
